cases = rnorm(10,30,2)
controls = rnorm(10,30,2)
t.test(cases,controls)

set.seed(100)
pvals <- replicate(2000,{
    cases = rnorm(10,30,2)
    controls = rnorm(10,30,2)
    t.test(cases,controls)$p.val
})
head(pvals)
hist(pvals)
x<-seq(0,1,0.1)
y<-sapply(x,function(x){mean(pvals<x)})
plot(x,y)
abline(0,1,col="red")
