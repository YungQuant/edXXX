set.seed(100)
B=1000
sigp<-replicate(B,{
    pvals <- replicate(20,{
        cases = rnorm(10,30,2)
        controls = rnorm(10,30,2)
        t.test(cases,controls)$p.val
    })
    sum(pvals<=0.05)
})
head(sigp)
table(sigp) ##just for illustration
mean(plessthan)
hist(sigp)
mean(sigp)

#reject null hypothesis at least once
mean(sigp>0)
